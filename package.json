{
  "name": "linden-honey",
  "version": "1.2",
  "description": "REST API for the lyrics of Yegor Letov and his post-punk/psychedelic rock-band Grazhdanskaya Oborona (Civil Defense)",
  "main": "src/index.js",
  "scripts": {
    "start": "node src",
    "debug": "node --inspect src",
    "test": "./node_modules/.bin/mocha src/**/*.spec.js",
    "migrate": "./node_modules/.bin/migrate up -d $LH_DB_URL --md src/migrations --es6 --autosync true"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/alebabai/linden-honey.git"
  },
  "keywords": [
    "linden honey",
    "letov",
    "grob",
    "koajs",
    "mongo",
    "mongoose"
  ],
  "author": "Alexander Babai",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alebabai/linden-honey/issues"
  },
  "homepage": "https://github.com/alebabai/linden-honey",
  "dependencies": {
    "async-retry": "^1.2.1",
    "cheerio": "^1.0.0-rc.2",
    "encoding": "^0.1.12",
    "koa": "^2.5.0",
    "koa-logger": "^3.1.0",
    "koa-router": "^7.4.0",
    "migrate-mongoose": "^3.2.2",
    "mongoose": "^4.13.11",
    "nconf": "^0.10.0",
    "node-fetch": "^2.1.1",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.18.2",
    "mocha": "^5.0.4"
  },
  "engines": {
    "node": "10.13.0"
  }
}
